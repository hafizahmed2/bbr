<% booking = @model_object %>
<h3>New question submission</h3>

<table>
  <tr>
    <td>Name</td>
    <td><%= booking.user_name %></td>
  </tr>

  <tr>
    <td>Email</td>
    <td><%= booking.user_email %></td>
  </tr>

  <tr>
    <td>Question</td>
    <td><%= simple_format(booking.question) %></td>
  </tr>
</table>
